<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yusufboss.manobojatok" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MaterialDrawerTheme.Light.DarkToolbar.TranslucentStatus" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HealthCalculator"
            android:label="@string/title_activity_health_calculator"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".MotherHealthInfo"
            android:label="@string/title_activity_mother_health_info"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".ChildHealthInfo"
            android:label="@string/title_activity_child_health_info"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Hospital"
            android:label="@string/title_activity_hospital"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".EmergencyContact"
            android:label="@string/title_activity_emergency_contact"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Vaccination"
            android:label="@string/title_activity_vaccination"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
